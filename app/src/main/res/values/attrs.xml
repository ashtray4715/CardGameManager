<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="VerticalMenuItem">
        <attr name="vmi_text" format="string" />
        <attr name="vmi_image" format="reference" />
    </declare-styleable>
    <declare-styleable name="FilledSingleButton">
        <attr name="fsb_text" format="string" />
    </declare-styleable>
    <declare-styleable name="StrokedSingleButton">
        <attr name="ssb_text" format="string" />
    </declare-styleable>
    <declare-styleable name="BaseActionBar">
        <attr name="actionbar_title" format="string" />
        <attr name="actionbar_back_btn_visibility" format="boolean" />
        <attr name="actionbar_back_btn_icon" format="reference" />
        <attr name="actionbar_menu_item_icon" format="reference" />
        <attr name="actionbar_menu_item_visibility" format="boolean" />
    </declare-styleable>
    <declare-styleable name="BasicInputField">
        <attr name="bif_title" format="string" />
        <attr name="bif_title_visibility" format="boolean" />
        <attr name="bif_text" format="string" />
        <attr name="bif_hint" format="string" />
        <attr name="bif_image" format="reference" />
        <attr name="bif_image_visibility" format="boolean" />
        <attr name="bif_input_type" format="enum">
            <enum name="TEXT" value="0" />
            <enum name="ONLY_POSITIVE_NUMBER" value="1" />
            <enum name="ALL_KIND_OF_NUMBERS" value="2" />
            <enum name="ERROR" value="3" />
        </attr>
    </declare-styleable>
</resources>